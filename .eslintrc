{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "plugins": ["prettier", "jest"],
  "extends": ["eslint:recommended", "prettier", "plugin:jest/recommended"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
